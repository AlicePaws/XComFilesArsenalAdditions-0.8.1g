          unit.getRuleArmor armorUnit;
          armorUnit.getTag flareLivingTag Tag.ITAG_ILLUMINATION_LIVING;
          armorUnit.getTag illuminationDuration Tag.ITAG_ILLUMINATION_DURATION;
          armorUnit.getTag proximityLivingTag Tag.PTAG_PROXIMITY_LIVING;
          if and eq flareLivingTag 1 eq currSide 0;
            unit.getInventoryItem smokeItem 3;
            unit.getInventoryItem flareItem 2;

            unit.getRightHandWeapon flareRightHand;
            unit.getLeftHandWeapon flareLeftHand;

            # debug_log illuminationDuration;
            mul illuminationDuration 2; #compensating
            # debug_log flareItem;
            # debug_log smokeItem;
            flareItem.setFuseEnabled 1;
            flareItem.setFuseTimer illuminationDuration;

            flareLeftHand.setFuseEnabled 1;
            flareLeftHand.setFuseTimer illuminationDuration;
            flareRightHand.setFuseEnabled 1;
            flareRightHand.setFuseTimer illuminationDuration;

            smokeItem.setFuseEnabled 1;
            smokeItem.setFuseTimer 0;
            # debug_log "This armor has a flare value CREATEUNIT VERSION ";
            unit.addStun 25;
            unit.disableIndicators;
          else and eq proximityLivingTag 1 eq currSide 0;
            unit.getInventoryItem proxyItem 0;
            # debug_log proxyItem;
            proxyItem.setFuseEnabled 1;
            proxyItem.setFuseTimer 0;
            # debug_log "This armor has a proxy value CREATEUNIT VERSION ";
            unit.addStun 25;
            unit.disableIndicators;
          end;
          return;
